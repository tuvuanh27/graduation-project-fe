{
  "name": "graduation-project-fe",
  "private": true,
  "version": "0.0.1",
  "description": "Graduation Project Frontend using React, Vite and Typescript",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "./deploy.sh",
    "lint": "yarn run eslint . --ext .js,.jsx,.ts,.tsx",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "yarn pretty",
      "yarn lint",
      "git add ."
    ]
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.0.1",
    "@walletconnect/qrcode-modal": "^1.7.1",
    "@walletconnect/web3-provider": "^1.7.1",
    "axios": "^1.3.4",
    "buffer": "^6.0.3",
    "immer": "^9.0.19",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.39.3",
    "react-router": "^6.8.2",
    "react-router-dom": "^6.8.2",
    "sass": "^1.59.2",
    "util": "^0.12.4",
    "web3": "^1.7.0",
    "zustand": "^4.3.5"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@metamask/providers": "^8.1.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.10.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.5.5",
    "vite": "^2.8.0"
  }
}
